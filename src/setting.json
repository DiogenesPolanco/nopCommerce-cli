{
    "name": "nopCommerce-cli",
    "shortname": "nopcli",
    "version": "2.1.3",
    "description": "nopCommerce CLI is a development tool for build plugins for nopCommerce Solutions.",
    "app": [{
        "plugin": [{
            "group": [{
                "name": [
                    "Widgets"
                ],
                "default": [
                    "Widgets"
                ]
            }],
            "generic": [{
                "name": "NopCliGeneric"
            }]
        }],
        "source": [{
            "slPath": "${process.mainModule.paths[2]}/nopcli",
            "srcPluginName": "Nop.Plugin.${yargs.argv.g}.NopCliGeneric",
            "pluginName": "Nop.Plugin.${yargs.argv.g}.${yargs.argv.p}",
            "pluginsPath": "${slPath}/Plugins/${pluginName}",
            "getNopVersion": "${slPath}/Libraries/Nop.Services/Plugins/Samples/uploadedItems.json"
        }]
    }],
    "dotnet": [{
        "framework": [
            "netcoreapp2.2",
            "netcoreapp3.1",
            "net5.0",
            "net6.0"
        ],
        "sdk": [
            "3.1.19"
        ],
        "runtimes": [
            "3.1.19"
        ]
    }],
    "nopcommerce":[{
        "version": [{ 
            "available": [ 
                "4.20", 
                "4.30", 
                "4.40", 
                "4.50"
            ],
            "default": "4.30",
            "supported": [
                "4.30", 
                "4.40"
            ]
        }]
        
    }],
    "docker": []
    
}