{
    "name": "nopCommerce-cli",
    "shortName": "nopcli",
    "description": "nopCommerce CLI (NopCli) is a bilder nopCommerce plugin, and it gives you the opportunity to start a plugin in less than 5 minutes!",
    "app": [{
        "plugin": [{
            "group": [{
                "name": [
                    "DiscountRoles",
                    "Misc",
                    "Payments",
                    "Shipping",
                    "Widgets"
                ],
                "default": [
                    "Widgets"
                ]
            }],
            "genericName": "NopCliGeneric"
        }]
    }],
    "dotnet": [{
        "framework": [
            "netcoreapp2.2",
            "netcoreapp3.1",
            "net5.0",
            "net6.0"
        ],
        "sdk": [
            "3.1.19"
        ],
        "runtimes": [
            "3.1.19"
        ]
    }],
    "nopCommerce":[{
        "version": [{ 
            "available": [ 
                "4.20", 
                "4.30", 
                "4.40", 
                "4.50"
            ],
            "default": "4.30",
            "supported": [
                "4.30", 
                "4.40"
            ]
        }]
        
    }],
    "docker": []
    
}